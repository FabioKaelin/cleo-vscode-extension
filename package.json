{
	"name": "cleo",
	"displayName": "Cleo",
	"description": "Cleo extension for Visual Studio Code",
	"version": "0.0.8",
	"publisher": "Cleo",
	"engines": {
		"vscode": "^1.65.0"
	},
	"main": "./out/extension.js",
	"activationEvents": [
		"onCommand:extension.viewJson"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/FabioKaelin/cleo-vscode-extension.git"
	},
	"categories": [
		"Themes",
		"Formatters",
		"Other",
		"Visualization"
	],
	"contributes": {
		"commands": [
			{
				"command": "extension.viewJson",
				"title": "%extension.viewJson.title%"
			}
		],
		"viewsWelcome": [
			{
				"view": "cleoView1",
				"contents": "Set your Theme to a Cleo-Theme!!!\n[Select a Cleo Theme](command:workbench.action.selectTheme)\n[Select Cleo Icon Theme](command:workbench.action.selectIconTheme)\n[Open Folder](command:vscode.openFolder)\n[Open File](command:workbench.action.files.openFile)"
			}
		],
		"keybindings": [
			{
				"key": "ctrl+alt+c",
				"command": "type",
				"args": {
					"text": "Cleo"
				},
				"when": "editorTextFocus"
			},
			{
				"command": "extension.viewJson",
				"key": "ctrl+j",
				"mac": "cmd+j",
				"when": "resourceLangId == json"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"when": "resourceLangId == json",
					"command": "extension.viewJson",
					"group": "navigation@-10"
				}
			],
			"editor/title/context": [
				{
					"when": "resourceLangId == json",
					"command": "extension.viewJson",
					"group": "navigation@-10"
				}
			],
			"editor/context": [
				{
					"when": "resourceLangId == json",
					"command": "extension.viewJson",
					"group": "navigation@-10"
				}
			],
			"view/title": [
				{
					"command": "workbench.action.selectTheme",
					"when": "view == cleoView1",
					"group": "navigation"
				}
			]
		},
		"views": {
			"cleoView": [
				{
					"id": "cleoView1",
					"name": "Cleo"
				}
			]
		},
		"viewsContainers": {
			"activitybar": [
				{
					"icon": "cat.svg",
					"id": "cleoView",
					"title": "Cleo"
				}
			]
		},
		"iconThemes": [
			{
				"id": "material-icon-theme",
				"label": "Cleo Icon Theme",
				"path": "./dist/material-icons.json",
				"_watch": true
			}
		],
		"themes": [
			{
				"id": "cleo default",
				"label": "--Cleo Default",
				"uiTheme": "vs-dark",
				"path": "./themes/cleoDefault-color-theme.json"
			},
			{
				"id": "cleo blue",
				"label": "-Cleo Blue",
				"uiTheme": "vs-dark",
				"path": "./themes/cleoBlue-color-theme.json"
			},
			{
				"id": "cleo green",
				"label": "-Cleo Green",
				"uiTheme": "vs-dark",
				"path": "./themes/cleoGreen-color-theme.json"
			},
			{
				"id": "cleo red",
				"label": "-Cleo Red",
				"uiTheme": "vs-dark",
				"path": "./themes/cleoRed-color-theme.json"
			},
			{
				"id": "all dark",
				"label": "--Dark",
				"uiTheme": "vs-dark",
				"path": "./themes/alldark-color-theme.json"
			}
		]
	},
	"icon": "https://github.com/FabioKaelin/cleo-vscode-extension/blob/main/cleo%20Transparent.png?raw=true",
	"keywords": [
		"theme",
		"blue",
		"green",
		"cleo",
		"Json",
		"View"
	],
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "^14.14.37",
		"@types/vscode": "^1.34.0",
		"@typescript-eslint/parser": "^4.22.1",
		"@vscode/test-electron": "^2.1.2",
		"eslint": "^7.23.0",
		"eslint-config-prettier": "^8.1.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-prettier": "^3.3.1",
		"gulp": "^4.0.2",
		"lint-staged": "^11.0.0",
		"mocha": "^9.2.1",
		"prettier": "^2.2.1",
		"typescript": "^4.6.3",
		"vsce": "^2.6.7",
		"vscode-nls-dev": "^4.0.0"
	},
	"__metadata": {
		"id": "baa8d1d7-cefd-4425-9394-959f30f50413",
		"publisherDisplayName": "Cleo",
		"publisherId": "c7d46808-7ea7-4e1e-9233-adaeed9e576f",
		"isPreReleaseVersion": false
	},
	"dependencies": {
		"vscode-nls": "^5.0.0"
	}
}